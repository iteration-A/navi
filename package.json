{
	"name": "navi",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"@types/axios": "^0.14.0",
		"@types/ink-divider": "^2.0.2",
		"@types/ora": "^3.2.0",
		"@types/uuid": "^8.3.3",
		"axios": "^0.24.0",
		"ink": "^3.2.0",
		"ink-divider": "^3.0.0",
		"meow": "^9.0.0",
		"module-alias": "^2.2.2",
		"react": "^17.0.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/react": "^17.0.37",
		"ava": "^3.15.0",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.5.2",
		"xo": "^0.39.1"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"_moduleAliases": {
		"@components": "dist/components",
		"@seed": "dist/seed",
		"@constants": "dist/constants",
		"@hooks": "dist/hooks",
		"@services": "dist/services"
	}
}
